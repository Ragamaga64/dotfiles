* {
    font-family: "JetBrainsMono Nerd Font";
    font-size: 11px;
    border: none;
}

window#waybar {
    background: rgba(46, 52, 64, 0.75);
    border-radius: 14px;
    height: 24px;
    margin: 0 50px 12px 50px;
}

#workspaces button {
    padding: 0 6px;
    color: #D8DEE9;
}

#workspaces button.active {
    background: #81A1C1;
    color: #2E3440;
    border-radius: 10px;
}

#clock, #battery, #network, #pulseaudio {
    padding: 0 8px;
    color: #ECEFF4;
}

